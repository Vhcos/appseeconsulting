+
+  // 2) Fallback: algún SURVEY que tenga keys B1./B2. (por si el título cambió)
+  const byKeys = await prisma.questionSet.findFirst({
+    where: {
+      engagementId,
+      kind: QuestionSetKind.SURVEY,
+      isActive: true,
+      OR: [
+        { questions: { some: { key: { startsWith: "B1." } } } },
+        { questions: { some: { key: { startsWith: "B2." } } } },
+      ],
+    },
+    include: { questions: { orderBy: { order: "asc" } } },
+  });
+  if (byKeys) return byKeys;
+
+  // 3) Último recurso: cualquier SURVEY activo
+  return prisma.questionSet.findFirst({
+    where: { engagementId, kind: QuestionSetKind.SURVEY, isActive: true },
+    include: { questions: { orderBy: { order: "asc" } } },
+  });
