+async function getInternalSurveyQuestionSet(engagementId: string) {
+  // 1) Preferimos el set correcto por título
+  const byTitle = await prisma.questionSet.findFirst({
+    where: {
+      engagementId,
+      kind: QuestionSetKind.SURVEY,
+      titleEs: "Encuesta interna",
+      isActive: true,
+    },
+    include: { questions: { orderBy: { order: "asc" } } },
+  });
